{
    "compilerOptions": {
      "target": "ESNext",
      "lib": ["dom", "dom.iterable", "esnext"],
      "types": ["vite/client"],
      "isolatedModules": true,
      "baseUrl": "src",
      "jsx": "react-jsx",
      "skipLibCheck": true,
      "esModuleInterop": true,
      "strict": false,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "allowSyntheticDefaultImports": true
    },
    "include": ["src"]
  }
  